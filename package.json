{
  "name": "@boses/skillink",
  "version": "0.0.0",
  "description": "统一 AI Skills 管理工具 - 像 pnpm 一样链接到各 AI 工具目录",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "skillink": "./dist/bin/skillink.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/bin/skillink.js",
    "lint": "tsc --noEmit",
    "check": "eslint",
    "format": "prettier --write src"
  },
  "keywords": [
    "ai",
    "skills",
    "cursor",
    "gemini",
    "cli",
    "sync",
    "symlink"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.18.2",
  "dependencies": {
    "chokidar": "^5.0.0",
    "commander": "^14.0.3",
    "fs-extra": "^11.3.3"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@eslint/markdown": "^7.5.1",
    "@inquirer/prompts": "^8.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.2.1",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}